<html>
  <head>
    <title><%= title %></title>
    <link rel='stylesheet' href='/stylesheets/style.css' />
    <script type="text/javascript" src="https://code.jquery.com/jquery-3.5.1.min.js" ></script>

    <link  href="javascripts/cropper.min.css" rel="stylesheet">
    <script src="javascripts/cropper.min.js"></script>
    <script src="javascripts/jquery-cropper.min.js"></script>

          <script src="https://cdn.rawgit.com/igorlino/elevatezoom-plus/1.1.20/src/jquery.ez-plus.js"></script>

    <link rel="stylesheet" href="https://maxcdn.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap.min.css" crossorigin="anonymous">
    <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.0.0/css/bootstrap-theme.min.css" crossorigin="anonymous">
    <script src="https://stackpath.bootstrapcdn.com/bootstrap/4.0.0/js/bootstrap.min.js"></script>
  </head>
  <body>
    <div>
      <div class="" style="padding-top:40px;" >
        <input type="hidden" id="refCurrentPage" value=""> </input>
        <input type="hidden" id="imgAngle" name="imgAngle" value="0"> </input>
         <div class="container">

             <div class="navBar1">
               <img src="/images/print.png" alt="print" onclick="print()" />
               <!--<img src="/images/save.png" alt="save" /> -->
               <img src="/images/download.png" alt="download" onclick="download()" />
               <img id="first-img" src="/images/firstpage.jpg" alt="first page" onclick="renderPage('first')" />
               <img id="previous-img" src="/images/previous.png" alt="previous" onclick="renderPage('prev')" />
               <div class="currentpageholder"><input type="textbox" id="currentpage" maxlength="3" value = "<%= currentpage %>" /></div>
               <img id="slash-img" src="/images/slashline.png" alt="slash" />
               <div class="totalpagesholder"><span id="totalpages"><%= totalPages %></span></div>
               <img id="next-img" src="/images/next.png" alt="next" onclick="renderPage('next')" />
               <img id="last-img" src="/images/lastpage.png" alt="last page" onclick="renderPage('last')" />
               <img src="/images/zoom-in.png" alt="zoom-in" onclick="zoomout()" />
               <img src="/images/zoom-out.jpg" alt="zoom-out" onclick="zoomin()" />
               <img src="/images/clockwise.png" alt="clockwise" onclick="drawRotated(90)" />
               <img src="/images/counter-clockwise.png" alt="counter-clockwise" onclick="drawRotated(-90)" />
               <img src="/images/rotate-180.png" alt="rotate 180 degree" onclick="drawRotated(180)" />
               <img src="/images/thumbnail.jpg" alt="thumbnail" onclick="renderThumbNail(null, 1)" />
             </div>
             <div class="navBar2">
                <img src="/images/fit-to-height.png" alt="fit to height" onclick="fittoheight()" />
                <img src="/images/fit-to-width.png" alt="fit to width" onclick="fittowidth()" />
                <img src="/images/fit-to-page.png" alt="fit to page" onclick="fittopage()" />
                <img src="/images/zoom-to-100percent.png" alt="zoom to 100 percent" onclick="zoomto100()" />
                <img src="/images/cropper.jpg" alt="crop image" onclick="crop()" />
                <img src="/images/magnifier.png" alt="zoom lens" onclick="zoomlens()" />
                <img src="/images/invert.png" alt="zoom lens" onclick="invertColorImage()" />
                <img src="/images/brightness.png" alt="brightness" onclick="addBrightness()" />
                <img src="/images/contrast.png" alt="contrast" onclick="addContrast()" />


            </div>

           <div id="imageContainer">
             <div id="mydiv"></div>
             <div id="thumbnaildiv"></div>

          </div>
         </div>

      </div>
    </div>
    <div id="cropdiv"> </div>

  </body>
  <script src="javascripts/zoomlens-1.0.1.min.js" type="text/javascript"></script>
    <script src="javascripts/myscript.js" type="text/javascript"></script>
    <script>

</script>

</html>
